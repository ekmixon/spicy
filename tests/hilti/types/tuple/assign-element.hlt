# @TEST-EXEC: hiltic -j %INPUT >output

module Foo {

import hilti;

global auto t1 = ("Hello!", True);

assert t1 == ("Hello!", True);
t1[0] = "Tach!";
t1[1] = False;
assert t1 == ("Tach!", False);

type X = struct {
    string s;
};

global tuple<x: strong_ref<X>, y: int<64>> t2;

t2.x = new X;
t2.x.s = "foo";
t2.y = 42;

hilti::print(t2);

assert t2.x.s == "foo";
assert t2.y == 42;
}
